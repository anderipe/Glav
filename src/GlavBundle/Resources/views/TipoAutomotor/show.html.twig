{% extends 'GlavBundle::layoutAdmin.html.twig' %}

{% block infoUserTop %}
    <h1>Tipo Automotor Registrado</h1>

      <table class="table table-bordered table-striped table-condensed">

        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
           
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('tipoautomotor') }}">
            Volver a la lista Tipo de Automotor
        </a>
    </li>
    <li>
        <a href="{{ path('tipoautomotor_edit', { 'id': entity.id }) }}">
            Editar Tipo de Automotor
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
