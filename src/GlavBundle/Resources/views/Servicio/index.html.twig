{% extends 'GlavBundle::layoutAdmin.html.twig' %}


{% block infoUserTop %}
    <h1>Servicio list</h1>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Hash</th>
                <th>Observacion</th>
                <th>Fecha_servicio</th>
                <th>Fecha_entrega</th>
                <th>Estado</th>
                <th>Fecha</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('servicio_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.hash }}</td>
                <td>{{ entity.observacion }}</td>
                <td>{% if entity.fechaservicio %}{{ entity.fechaservicio|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechaentrega %}{{ entity.fechaentrega|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.estado }}</td>
                <td>{% if entity.fecha %}{{ entity.fecha|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('servicio_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('servicio_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('servicio_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
