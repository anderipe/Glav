{% extends 'GlavBundle::layoutAdmin.html.twig' %}

{% block infoUserTop %}
    <h1>Tipo de Servicio</h1>

     <table class="table table-bordered table-striped table-condensed">

        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('tiporubro') }}">
            Volver a la Lista Tipo de Servicio
        </a>
    </li>
    <li>
        <a href="{{ path('tiporubro_edit', { 'id': entity.id }) }}">
            Editar Tipo de Servicio
        </a>
    </li>
    <li>{{ form(delete_form, {'attr': {'onclick': 'return confirm("Desea Eliminar el registro ?")'}}) }}</li>
</ul>
{% endblock %}
