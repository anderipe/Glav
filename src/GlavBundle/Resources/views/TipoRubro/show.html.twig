{% extends 'GlavBundle::layoutAdmin.html.twig' %}

{% block infoUserTop %}
    <h1>Tipo de Servicio</h1>

   <table class="table table-striped table-bordered bootstrap-datatable datatable">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Hash</th>
                <td>{{ entity.hash }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('tiporubro') }}">
            Volver a la Lista Tipo de Servicio
        </a>
    </li>
    <li>
        <a href="{{ path('tiporubro_edit', { 'id': entity.id }) }}">
            Editar Tipo de Servicio
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
